{
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "plugins": [
        "@trivago/prettier-plugin-sort-imports",
        "prettier-plugin-organize-imports",
        "prettier-plugin-jsdoc"
    ],
    "tsdoc": true,
    "jsdocCommentLineStrategy": "multiline",
    "importOrder": [
        "^@girs/(.*)$",
        "^@/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
